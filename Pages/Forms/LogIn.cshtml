@page
@model Nevada.Pages.Forms.LogInModel
@{
}
<div class="text-center container">
    <h1 class=""></>Log in</h1>
    <form method="post" class="mx-auto mt-5 mb-5 px-5 pt-5 pb-5 rounded-5 bg-light col-4">
        <div class=" mt-3 mb-3">
            <input type="text" asp-for="User.Email" placeholder="Type your email" />
        </div>
        <br />
        <div class=" mt-3 mb-3">
            <input type="password" asp-for="User.Password" placeholder="Type your password" />
        </div>
        <br />
        <div class=" mt-3 mb-3">
            <button class="btn btnEdit1 btnEdit mt-1 " type="submit">Log in</button>
        </div>
    </form>
</div>